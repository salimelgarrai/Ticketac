<!DOCTYPE html>
<html>
<head>
  <title>Ticketac</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
</head>
<body>
  <%- include('navbar') %>


  <div class="container container-fluid d-flex justify-content-center align-items-center">
    <div class="row">
    <div class="col-12 col-lg-6">
      <form class="d-flex flex-column justify-content-center align-items-center" action="index.html" method="post">
        <h5>Sign-in :</h5>
      <input class="form-control" type="text" placeholder="Email" required>
      <input class="form-control" type="password" placeholder="Password" required>
      <input type="submit" class="btn btn-danger mt-2" value="Confirm">
      </form>
    </div>
    <div class="col-12 col-lg-6">
      <form class="d-flex flex-column justify-content-center align-items-center" action="index.html" method="post">
        <h5>Sign-up :</h5>
      <input class="form-control" type="text" placeholder="First Name" required>
      <input class="form-control" type="text" placeholder="Last Name" required>
      <input class="form-control" type="email" placeholder="Email" required>
      <input class="form-control" type="password" placeholder="Password" required>
      <input type="submit" class="btn btn-danger mt-2" value="Confirm">
    </form>
    </div>
    </div>

  </div>




  <%- include('footer') %>
  <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
</body>
</html>
