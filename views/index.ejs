      <div class="row d-flex flex-row justify-content-center pt-5 pb-5 mt-5 mt-5 form-row">
        <h5 class="text-white pb-3">One-way ticket :</h5>
        <form class="d-flex flex-row justify-content-center align-items-center mb-4" action="/home/travel" method="post">
          <input class="form-control m-2 p-2" type="text" placeholder="From: Paris, Marseille..." name="depart" required>
          <input class="form-control m-2 p-2" type="text" placeholder="To: Lille, Nantes..." name="arrivÃ©e" required>
          <input class="form-control m-2 p-2" type="date" name="date" value="2018-11-20" required>
          <input type="submit" class="btn btn-danger m-2" value="GO">
        </form>
        <img class="img-fluid p-5" style="max-width: 320px" src="images/landmark.png" alt="landmark">
      </div>
    </div>
    <%- include('footer') %>
  </div>




